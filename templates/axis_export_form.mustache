{{!
    @template assignfeedback_editpdfplus/axis_export_form

    Admin template for export one axis.

    Context variables required for this template:
    * contextid - Context id
    * title - Title of the page
    * render - form render for removing the axis
    * id - form id
}}

<div class="card-header">{{title}}</div>
<div class="card-body text-primary">
    {{{form.render}}}
    <button type="button" class="btn btn-primary" id="axisExportSubmit">{{# str }} adminaxisexport_ok, assignfeedback_editpdfplus {{/ str }}</button>
    <button type="button" class="btn" id="axisExportCancel">{{# str }} adminaxisexport_ko, assignfeedback_editpdfplus {{/ str }}</button>
</div>
